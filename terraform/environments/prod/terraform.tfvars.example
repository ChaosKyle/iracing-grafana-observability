# Grafana Cloud settings
grafana_cloud_url = "https://example.grafana.net"
grafana_cloud_api_key = "your-grafana-cloud-api-key"
grafana_cloud_org_id = "123456"
stack_slug = "your-stack-slug"
grafana_admin_user = "admin"
grafana_admin_pass = "strong-password"

# Metrics publishing
grafana_metrics_publisher_id = "12345"
grafana_metrics_api_key = "your-metrics-api-key"

# Docker settings
docker_host = "unix:///var/run/docker.sock"

# Prometheus settings
prometheus_data_path = "/var/lib/prometheus"
prometheus_port = 9090

# PostgreSQL settings
postgres_user = "postgres"
postgres_password = "strong-postgres-password"
postgres_database = "iracing_data"